<?xml version="1.0" encoding="UTF-8"?>
<project
  xmlns="http://maven.apache.org/POM/4.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd"
>
  <parent>
    <groupId>org.mortbay.jetty</groupId>
    <artifactId>jetty-parent</artifactId>
    <version>8</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.mortbay.jetty</groupId>
  <artifactId>jetty-project</artifactId>
  <packaging>pom</packaging>
  <version>7.0.0.0-SNAPSHOT</version>
  <name>Jetty Project</name>

  <properties>
    <atomikos-version>3.2.3</atomikos-version>
    <jetty-version>7.0.0.M0</jetty-version>
    <jsp-2-1-version>9.1.1.B60.25.p0</jsp-2-1-version>
  </properties>

  <scm>
    <connection>scm:svn:https://svn.codehaus.org/jetty/jetty/trunk/jetty-project</connection>
    <developerConnection>scm:svn:https://svn.codehaus.org/jetty/jetty/trunk/jetty-project</developerConnection>
    <url>scm:svn:https://svn.codehaus.org/jetty/jetty/trunk/jetty-project</url>
  </scm>

  <build>
    <defaultGoal>install</defaultGoal>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin
        </artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
          <verbose>false</verbose>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-release-plugin</artifactId>
        <configuration>
          <tagBase>https://svn.codehaus.org/jetty/jetty/tags</tagBase>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
