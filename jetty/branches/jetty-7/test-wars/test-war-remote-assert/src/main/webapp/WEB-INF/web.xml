<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://java.sun.com/xml/ns/javaee" 
  xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  id="WebApp_ID" 
  version="2.5">
  <display-name>RemoteAsserts to test jetty-remote-asserts from Webapp point of view</display-name>

  <servlet>
    <servlet-name>TestServlet</servlet-name>
    <servlet-class>org.mortbay.jetty.tests.webapp.remoteassert.TestSuiteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TestServlet</servlet-name>
    <url-pattern>/tests</url-pattern>
  </servlet-mapping>
</web-app>
