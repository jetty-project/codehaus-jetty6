grant {
  
  permission java.io.FilePermission "${test.resource.dir}${/}-", "read";
  
  permission java.util.PropertyPermission "line.separator", "read";
  
  
  permission java.net.SocketPermission "localhost:1024-", "connect,resolve,listen,accept";
  permission java.net.SocketPermission "[0:0:0:0:0:0:0:1]:1024-", "connect,resolve,listen,accept";
  permission java.util.PropertyPermission "org.eclipse.jetty.util.URI.charset", "read";


  permission java.lang.RuntimePermission "setContextClassLoader";
  permission java.lang.RuntimePermission "setIO";
  permission java.lang.RuntimePermission "setSecurityManager";
  permission java.security.SecurityPermission "getPolicy";
}